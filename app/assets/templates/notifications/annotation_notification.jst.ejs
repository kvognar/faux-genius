<a href="/#/articles/<%= article.id %>" class="notification-pane">
  <div class="row">
    <div class="col-md-1">
      <img src="<%= author.escape('image_url') %>" class="user-icon">
    </div>
    <div class="col-md-11">
      <%= author.escape('username') %> created an annotation on
      <span class="article-title">
        <%= _.escape(article.title) %>
      </span>
      <time class="timeago notification-timestamp">
        <%= $.timeago(new Date(notification.escape('created_at'))) %>
      </time>
    </div>
  </div>
</a>
<div class="source-container"></div>